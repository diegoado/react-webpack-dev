{
  "presets": [
    "env",
    "stage-0",
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["babel-plugin-webpack-alias", {
      "config": "./config/webpack.config.dev.js"
    }]
  ]
}
